{
    "name": "random-read",
    "version": "1",
    "manifest_version": 2,
    "description": "random-read chrome extension",
    "browser_action": {
        "default_icon": "testlogo.png"
    },

    "content_scripts": [{
        "matches": ["http://*/*", "https://*/*"],
        "js": [
            "jquery.js"
        ]
    }],
    "permissions": [
        "tabs",
        "https://*/*",
        "http://*/"
    ]
}